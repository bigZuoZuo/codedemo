{
  "name": "yzs-h5-mini-app-taro",
  "version": "1.0.0",
  "private": true,
  "description": "鸭嘴兽小程序",
  "templateInfo": {
    "name": "mobx",
    "typescript": true,
    "css": "none"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5-uat": "cross-env YZS_ENV=uat taro build --type h5",
    "build:h5-dev": "cross-env YZS_ENV=dev taro build --type h5",
    "build:h5-prod": "cross-env YZS_ENV=prod taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "cross-env YZS_ENV=dev taro build --type h5 --watch",
    "dev:h5-uat": "cross-env YZS_ENV=uat taro build --type h5 --watch",
    "dev:h5-prod": "cross-env YZS_ENV=prod taro build --type h5 --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "prepare": "husky install",
    "prettier": "prettier --write ./src/**/*.{ts,js,wxs,wxml,wxss,json}",
    "gen:api": "npx swagger-typescript-api -p https://carbon-backend-prod.run.fish/api/v1/projects/4030174/swagger.json -o ./src/api -n api.ts --axios --default-response=any",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook",
    "type-check": "tsc --noEmit"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.19.4",
    "@tarojs/components": "3.4.7",
    "@tarojs/plugin-framework-react": "3.4.7",
    "@tarojs/runtime": "3.4.7",
    "@tarojs/taro": "3.4.7",
    "axios": "0.21.1",
    "axios-miniprogram-adapter": "0.3.2",
    "babel-plugin-import": "1.13.5",
    "classnames": "2.3.1",
    "dayjs": "1.10.7",
    "lodash.findindex": "4.6.0",
    "mathjs": "10.1.0",
    "miniprogram-computed": "4.3.6",
    "mobx": "4.8.0",
    "mobx-logger": "0.7.1",
    "mobx-miniprogram": "4.13.2",
    "mobx-miniprogram-bindings": "2.1.4",
    "mobx-react": "6.1.4",
    "mockjs": "1.1.0",
    "nanoid": "3.1.30",
    "postcss-nesting": "10.1.3",
    "preact": "10.6.6",
    "pretty": "2.0.0",
    "qs": "6.10.3",
    "react-three-fbx-for-pyt": "1.0.8",
    "react-vant": "2.0.0-alpha.36",
    "sm-crypto": "0.3.8",
    "swiper": "8.1.3",
    "taro-axios": "1.1.1",
    "taro-ui": "3.1.0-beta.2",
    "taro-virtual-list": "1.0.13",
    "umtrack-wx": "2.7.1",
    "weui-miniprogram": "1.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.8.0",
    "@storybook/addon-actions": "6.4.18",
    "@storybook/addon-essentials": "6.4.18",
    "@storybook/addon-links": "6.4.18",
    "@storybook/addon-viewport": "6.4.19",
    "@storybook/preact": "6.4.18",
    "@storybook/preset-create-react-app": "4.0.1",
    "@storybook/react": "6.4.19",
    "@tarojs/cli": "3.4.2",
    "@tarojs/mini-runner": "3.4.1",
    "@tarojs/webpack-runner": "3.4.1",
    "@types/mockjs": "1.0.6",
    "@types/react": "17.0.2",
    "@types/umtrack-wx": "2.3.1",
    "@types/webpack-env": "1.13.6",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "ali-oss-publish": "0.4.0",
    "babel-loader": "8.2.3",
    "babel-plugin-react-directives": "2.0.1",
    "babel-plugin-transform-taroapi": "3.4.1",
    "babel-preset-taro": "3.4.1",
    "copy-to-clipboard": "3.3.1",
    "core-js": "3.21.0",
    "cross-env": "7.0.3",
    "eslint": "6.8.0",
    "eslint-config-taro": "3.4.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-react": "7.8.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-saver": "2.0.5",
    "html2canvas": "1.4.1",
    "husky": "^8.0.1",
    "lint-staged": "12.1.2",
    "postcss": "8.4.6",
    "postcss-pxtransform": "3.4.1",
    "prettier": "2.2.1",
    "qrcode.react": "3.1.0",
    "react-infinite-scroller": "^1.2.6",
    "stylelint": "14.5.3",
    "stylelint-config-standard": "25.0.0",
    "typescript": "4.1.3",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,wxs,wxml,wxss,json}": [
      "prettier --write",
      "git add"
    ],
    "*.css": "stylelint --fix"
  }
}
